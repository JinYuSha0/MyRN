{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"],
      "@apis/*": ["src/apis/*"],
      "@assets/*":["src/assets/*"],
      "@components/*":["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@navigators/*": ["src/navigators/*"],
      "@registry/*": ["src/registry/*"],
      "@screens/*": ["src/screens/*"],
      "@types/*":["src/types/*"],
      "@utils/*":["src/utils/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}